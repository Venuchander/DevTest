{
    "builds": [
        {
            "src": "DevTest/wsgi.py",
            "use": "@vercel/python",
            "config": { "maxLambdaSize": "15mb", "runtime": "python3.12" }
        }
    ],
    "routes": [
        {
            "src": "/static/(.*)",
            "dest": "DevTest/staticfiles/$1"
        },
        {
            "src": "/(.*)",
            "dest": "DevTest/wsgi.py"
        }
    ],
    "static": {
        "directory": "DevTest/staticfiles"
    }
}
